{
  "name": "youtube-audio-downloader-workspace",
  "version": "1.0.0",
  "description": "Modern web application for downloading high-quality audio from YouTube videos",
  "private": true,
  "workspaces": [
    "frontend",
    "backend"
  ],
  "scripts": {
    "dev": "concurrently \"npm run frontend\" \"npm run backend\"",
    "frontend": "cd frontend && npm run dev",
    "backend": "cd backend && npm start",
    "setup": "./setup.sh",
    "start": "./start.sh",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "keywords": ["youtube", "audio", "downloader", "mp3", "react", "vite", "node"],
  "author": "Your Name",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/youtube-audio-downloader.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/youtube-audio-downloader/issues"
  }
}
